{
  "name": "com.reactnative.mono.template",
  "version": "0.0.1",
  "buildNumber": "1",
  "scripts": {
    "web": "cross-env EXPO_NO_DOTENV=1 expo start --web",
    "xcode": "xed -b ios",
    "doctor": "npx expo-doctor@latest",
    "start": "cross-env EXPO_NO_DOTENV=1 expo start",
    "prebuild": "cross-env EXPO_NO_DOTENV=1 expo prebuild",
    "android": "cross-env EXPO_NO_DOTENV=1 expo run:android",
    "ios": "cross-env EXPO_NO_DOTENV=1 expo run:ios",
    "build:ios": "cross-env APP_ENV=development EXPO_NO_DOTENV=1 eas build --profile development --platform ios",
    "build:android": "cross-env APP_ENV=development EXPO_NO_DOTENV=1 eas build --profile development --platform android",
    "start:stag": "cross-env APP_ENV=staging yarn start",
    "prebuild:stag": "cross-env APP_ENV=staging yarn prebuild",
    "android:stag": "cross-env APP_ENV=staging yarn android",
    "ios:stag": "cross-env APP_ENV=staging yarn ios",
    "build:ios:stag": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 eas build --profile staging --platform ios",
    "build:android:stag": "cross-env APP_ENV=staging EXPO_NO_DOTENV=1 eas build --profile staging --platform android",
    "start:prod": "cross-env APP_ENV=production yarn start",
    "prebuild:prod": "cross-env APP_ENV=production yarn prebuild",
    "android:prod": "cross-env APP_ENV=production yarn android",
    "ios:prod": "cross-env APP_ENV=production yarn ios",
    "build:ios:prod": "cross-env APP_ENV=production EXPO_NO_DOTENV=1 eas build --profile production --platform ios",
    "build:android:prod": "cross-env APP_ENV=production EXPO_NO_DOTENV=1 eas build --profile production --platform android",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "extract": "lingui extract --clean",
    "compile": "lingui compile --typescript",
    "prepare": "husky",
    "generate-colors": "node scripts/generate-colors.js",
    "translate": "lingui extract --clean && ./scripts/auto-translate.sh && lingui compile --typescript"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@formatjs/intl-locale": "^4.2.11",
    "@formatjs/intl-numberformat": "^8.15.4",
    "@formatjs/intl-pluralrules": "^5.4.4",
    "@gorhom/bottom-sheet": "^5",
    "@legendapp/list": "^1.0.11",
    "@lingui/core": "^5.3.1",
    "@lingui/macro": "^5.3.1",
    "@lingui/react": "^5.3.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-firebase/analytics": "^22.0.0",
    "@react-native-firebase/app": "^22.0.0",
    "@react-native-firebase/firestore": "^22.0.0",
    "@react-native-firebase/messaging": "^22.0.0",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@shopify/flash-list": "^1.8.0",
    "@shopify/restyle": "^2.4.5",
    "@tanstack/query-async-storage-persister": "^5.75.0",
    "@tanstack/react-query": "^5.75.2",
    "@tanstack/react-query-persist-client": "^5.75.2",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "expo": "^53.0.8",
    "expo-blur": "~14.1.4",
    "expo-build-properties": "~0.14.6",
    "expo-clipboard": "~7.1.4",
    "expo-constants": "~17.1.6",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.7",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-localization": "~16.1.5",
    "expo-notifications": "~0.31.1",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.7",
    "html-entities": "^2.6.0",
    "i18n-js": "^4.5.1",
    "lodash.debounce": "^4.0.8",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-fast-compare": "^3.2.2",
    "react-hook-form": "^7.56.1",
    "react-native": "0.79.2",
    "react-native-device-info": "^14.0.4",
    "react-native-error-boundary": "^2.0.0",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-controller": "^1.17.1",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@lingui/babel-plugin-lingui-macro": "^5.3.1",
    "@lingui/cli": "^5.3.1",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@types/jest": "^29.5.12",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.1",
    "eslint-plugin-lingui": "^0.10.1",
    "husky": "^9.1.7",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.5",
    "lint-staged": "^15.5.1",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*{.js,.jsx,.ts,.tsx}": [
      "npx lingui extract --clean",
      "npx lingui compile --typescript",
      "git add src/locale/locales"
    ]
  },
  "private": true
}
